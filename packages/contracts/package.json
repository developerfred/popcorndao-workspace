{
  "name": "@popcorn/contracts",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build": "tsc",
    "dev": "yarn hardhat node",
    "dev:deploy": "yarn hardhat dev:deploy --network localhost",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "clean": "rimraf ./cache & rimraf ./artifacts",
    "compile": "yarn clean && hardhat compile",
    "postinstall": "yarn compile",
    "docify": "node docify.js"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@types/chai": "^4.2.16",
    "@types/node": "^14.14.41",
    "ethers": "^5.0.31",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^3.4.0",
    "@popcorn/utils": "*",
    "@typechain/ethers-v5": "^7.0.0",
    "@typechain/hardhat": "^2.0.0",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "bluebird": "^3.7.2",
    "chai": "^4.3.0",
    "ethereum-waffle": "^3.2.2",
    "hardhat": "^2.0.10",
    "merkle-tree-solidity": "^1.0.8",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.5",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^5.0.0",
    "solidity-docgen": "0.3.11"
  },
